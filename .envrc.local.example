# Sensitive environment variables
# Copy this file to .envrc.local and update with your actual values

# API Keys
export OPENAI_API_KEY="your-openai-api-key"
export GEMINI_API_KEY="your-gemini-api-key"

# FreeSWITCH Firefly user password
export FIREFLY_PASSWORD="your-firefly-password"

# Kyivstar SIP Configuration (production)
export KYIVSTAR_SIP_USERNAME="your-kyivstar-number"
export KYIVSTAR_SIP_PASSWORD="your-kyivstar-password"
export KYIVSTAR_SIP_DOMAIN="voip.kyivstar.ua"
export KYIVSTAR_SIP_PORT="5060"
export KYIVSTAR_SIP_TRANSPORT="udp"

# SIP Provider Selection (uncomment to switch from default FreeSWITCH)
# export SIP_PROVIDER="kyivstar"